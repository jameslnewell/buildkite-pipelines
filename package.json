{
  "name": "@jameslnewell/buildkite-pipelines-root",
  "version": "3.22.0",
  "private": true,
  "author": "James Newell <gday@jameslnewell.dev>",
  "description": "Generate Buildkite pipelines from code.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jameslnewell/buildkite-pipelines.git"
  },
  "bugs": {
    "url": "https://github.com/jameslnewell/buildkite-pipelines/issues"
  },
  "homepage": "https://github.com/jameslnewell/buildkite-pipelines",
  "packageManager": "pnpm@8.6.6",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@jameslnewell/jest-preset": "^3.0.0",
    "@jameslnewell/prettier-config": "^1.1.0",
    "@jest/types": "^27.5.1",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.15.1",
    "@swc/jest": "^0.2.20",
    "@swc/register": "^0.1.10",
    "@types/debug": "^4",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.14",
    "@types/prettier": "^2.4.4",
    "@types/resolve": "^1.20.1",
    "@types/yargs": "^17.0.10",
    "chokidar": "^3.5.3",
    "cpx": "^1.5.0",
    "eslint": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-pnp-resolver": "^1.2.2",
    "json-schema-to-typescript": "^13.0.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.7.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "@jameslnewell/jest-preset",
    "resolver": "jest-pnp-resolver",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "prettier": "@jameslnewell/prettier-config",
  "scripts": {
    "fix:formatting": "prettier --write .",
    "check": "pnpm run check:formatting && pnpm run check:typing",
    "check:formatting": "prettier --check .",
    "check:typing": "pnpm -r --parallel run check:typing",
    "prepare": "husky install",
    "build": "pnpm -r run build",
    "build:pages": "pnpm --filter @jameslnewell/buildkite-pipelines run build:pages",
    "test": "pnpm -r run test",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "license": "MIT"
}
